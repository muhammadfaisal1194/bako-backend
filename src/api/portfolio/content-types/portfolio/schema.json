    "link": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "overview": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "clientName": {
      "type": "string",
      "required": true
    }
  }
